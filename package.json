{
  "name": "video.js",
  "description": "An HTML5 and Flash video player with a common API and skin for both.",
  "version": "6.6.2",
  "main": "./dist/video.cjs.js",
  "style": "./dist/video-js.css",
  "copyright": "Copyright Brightcove, Inc. <https://www.brightcove.com/>",
  "license": "Apache-2.0",
  "keywords": [
    "flash",
    "html5",
    "player",
    "video",
    "videojs"
  ],
  "homepage": "http://videojs.com",
  "author": "Steve Heffernan",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:js": "npm-run-all -p build:js:*",
    "build:js:prod": "webpack --config scripts/webpack.config.js --mode production",
    "build:js:dev": "webpack --config scripts/webpack.config.js --mode development",
    "start": "webpack-dev-server --config scripts/webpack.config.js --mode development",
    "changelog": "conventional-changelog -p videojs -i CHANGELOG.md -s",
    "assets": "node build/assets.js",
    "docs": "npm run docs:lint && npm run docs:api",
    "jsdoc": "jsdoc",
    "predocs:api": "node -e \"var s=require('shelljs'),d=['docs/api'];s.rm('-rf',d);\"",
    "docs:api": "jsdoc -c .jsdoc.json",
    "postdocs:api": "node ./build/fix-api-docs.js",
    "netlify": "babel-node ./build/netlify-docs.js",
    "docs:lint": "remark -- './**/*.md'",
    "docs:fix": "remark --output -- './**/*.md'",
    "prepublish": "not-in-install && run-p build || in-install",
    "publish": "node build/gh-release.js",
    "prepush": "npm run lint -- --errors",
    "version": "node build/version.js && git add CHANGELOG.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/videojs/video.js.git"
  },
  "dependencies": {
    "global": "4.3.2",
    "safe-json-parse": "^4.0.0",
    "tsml": "^1.0.1",
    "video.js": "^6.6.3",
    "videojs-vtt.js": "^0.12.6",
    "xhr": "^2.4.1",
    "videojs-font": "2.1.0",
    "videojs-ie8": "1.1.2"
  },
  "devDependencies": {
    "bluebird": "^3.5.0",
    "cli-table": "^0.3.1",
    "conventional-changelog-cli": "^1.2.0",
    "conventional-changelog-videojs": "^3.0.0",
    "filesize": "^3.5.6",
    "gh-release": "^3.0.0",
    "husky": "^0.14.1",
    "in-publish": "^2.0.0",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.2",
    "karma": "2.0.0",
    "karma-browserstack-launcher": "^1.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-detect-browsers": "~2.2.3",
    "karma-firefox-launcher": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-qunit": "^1.2.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "klaw-sync": "^3.0.0",
    "load-grunt-tasks": "^3.1.0",
    "lodash": "^4.16.6",
    "markdown-table": "^1.0.0",
    "minimist": "^1.2.0",
    "npm-run": "^4.1.0",
    "npm-run-all": "^4.0.2",
    "proxyquireify": "^3.0.0",
    "qunitjs": "1.23.1",
    "remark-cli": "^4.0.0",
    "remark-lint": "^6.0.0",
    "remark-parse": "^4.0.0",
    "remark-stringify": "^4.0.0",
    "remark-toc": "^5.0.0",
    "remark-validate-links": "^7.0.0",
    "replace": "^0.3.0",
    "shelljs": "^0.7.5",
    "sinon": "^1.16.1",
    "tui-jsdoc-template": "^1.1.0",
    "uglify-js": "~2.8.8",
    "unified": "^6.1.5",
    "videojs-flash": "^2.0.0",
    "videojs-standard": "^6.0.1",
    "write-file-webpack-plugin": "^4.2.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0"
  },
  "vjsstandard": {
    "ignore": [
      "**/Gruntfile.js",
      "**/es5/**",
      "**/build/**",
      "!build/rollup.js",
      "**/dist/**",
      "**/docs/**",
      "**/lang/**",
      "**/sandbox/**",
      "**/test/api/**",
      "**/test/coverage/**",
      "**/test/karma.conf.js"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "qunitjs",
      "sinon",
      "webpack",
      "uglify-js"
    ]
  }
}
